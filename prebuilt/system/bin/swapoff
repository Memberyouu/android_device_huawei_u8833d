#!/system/bin/sh
# By xjljian
SWAPADD="loop7"
busybox swapoff /dev/block/$SWAPADD
busybox umount /cache
busybox umount /cust
busybox mke2fs -b 4096 /dev/block/mmcblk0p15
busybox mke2fs -b 4096 /dev/block/mmcblk0p16
busybox umount /cust
busybox umount /cache
busybox mount -t ext4 /dev/block/mmcblk0p15 /cache 
busybox rm -rf /cache/swap
busybox rmdir /data/property/swapp15-run
busybox rmdir /data/property/swapp16-run
exit
